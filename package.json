{
  "name": "viceversa-frontend-challenge",
  "version": "0.1.0",
  "private": true,
  "author": "schintu.enrico@gmail.com",
  "dependencies": {
    "echarts": "5.4.1",
    "echarts-for-react": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.2",
    "react-scripts": "5.0.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --coverage",
    "e2e:cypress:chrome": "cypress run --browser chrome --spec \"cypress/e2e/**/*\" --headless",
    "e2e:cypress:chrome-live": "cypress run --browser chrome --spec \"cypress/e2e/**/*\" --headed",
    "e2e:cypress:edge": "cypress run --browser edge --spec \"cypress/e2e/**/*\" --headless",
    "e2e:cypress:edge-live": "cypress run --browser edge --spec \"cypress/e2e/**/*\" --headed",
    "e2e:cypress:firefox": "cypress run --browser firefox --spec \"cypress/e2e/**/*\" --headless",
    "e2e:cypress:firefox-live": "cypress run --browser firefox --spec \"cypress/e2e/**/*\" --headed",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.21.0",
    "@cypress/webpack-dev-server": "3.3.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/node-logger": "6.5.16",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/dom": "9.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/echarts": "4.9.16",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.13",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.26",
    "babel-plugin-named-exports-order": "0.0.2",
    "cypress": "12.7.0",
    "cypress-mochawesome-reporter": "3.2.3",
    "eslint-plugin-cypress": "2.12.1",
    "jest": "29.4.3",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.1",
    "mochawesome-report-generator": "6.2.0",
    "prop-types": "15.8.1",
    "styled-components": "5.3.6",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  }
}
